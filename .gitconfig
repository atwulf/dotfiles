[user]
	name = Austin Wulf
	email = atwulf@gmail.com
[core]
	excludesfile = /Users/austinwulf/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[credential]
	helper = osxkeychain
[commit]
	template = /Users/austinwulf/.stCommitMsg
[alias]
	fm = git fetch && git merge
